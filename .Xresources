! ################ ROFI #################

! Appearance

rofi.width: 25ofi.color-enabled: true
rofi.color-window: #e6e6e6, #364759, #497391
rofi.color-normal: #e6e6e6, #002b36, #d7d7d7, #497391, #ffffff
rofi.color-active: #e6e6e6, #497391, #d7d7d7, #497391, #ffffff
rofi.color-urgent: #e6e6e6, #dc322f, #d7d7d7, #dc322f, #ffffff
rofi.separator-style: solid
rofi.lines: 5
rofi.colour-enabled: true
rofi.opacity: 100
rofi.hide-scrollbar: true
rofi.font: Hack 12


!Behaviour

rofi.terminal: urxvt
rofi.fuzzy: true

